{
  "name": "ngx-emoji",
  "version": "0.0.1",
  "description": "Components with Emoji for Angular 4+",
  "main": "./bundles/index.umd.js.min",
  "typings": "./bundles/index.d.ts",
  "scripts": {
    "rimraf": "rimraf",
    "clean:bundles": "npm run rimraf -- bundles",
    "clean:demo": "npm run rimraf -- demo",
    "clean:all": "npm run clean && npm run rimraf -- node_modules npm-debug.log .awcache package-lock.json",
    "clean": "npm run clean:bundles && npm run clean:demo",
    "prebuild:bundles": "npm run clean:bundles",
    "prebuild:demo": "npm run clean:demo",
    "build:bundles": "webpack --config webpack.config.js --progress --profile --bail",
    "build:demo": "webpack --config src/demo/webpack.config.js --progress --profile --bail",
    "build": "npm run build:bundles && npm run build:demo",
    "start:demo": "webpack-dev-server --config src/demo/webpack.config.js --progress --profile",
    "start": "npm run start:demo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arswarog/ngx-emoji.git"
  },
  "keywords": [
    "Emoji",
    "Angular",
    "Angular 4",
    "Text input"
  ],
  "author": "Arswarog <arswarog@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arswarog/ngx-emoji/issues"
  },
  "homepage": "https://github.com/arswarog/ngx-emoji#readme",
  "peerDependencies": {
    "@angular/core": "5.1.3"
  },
  "devDependencies": {
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "awesome-typescript-loader": "3.4.1",
    "core-js": "2.5.3",
    "dts-bundle": "0.7.3",
    "dts-bundle-webpack": "1.0.0",
    "html-webpack-plugin": "2.30.1",
    "rimraf": "2.6.2",
    "rxjs": "5.5.6",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.7.1",
    "zone.js": "0.8.19"
  }
}
